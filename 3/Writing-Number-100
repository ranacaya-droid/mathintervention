<script>
document.addEventListener('DOMContentLoaded', () => {

    // --- DATA & CONFIGURATION ---
    const positiveFeedback = ["Brilliant deduction!", "You've cracked the code!", "Excellent work, Detective!", "Great clue!", "Case closed!"];
    const negativeFeedback = ["Not quite, let's look at the clues again.", "Hmm, let's re-examine the evidence.", "Keep trying, Detective!"];
    
    // Fully populated data object for all 4 days.
    const lessonData = {
        day1: {
            title: "Case of the Missing Numbers",
            slides: [
                { type: 'title', title: 'Hook', content: "Detectives, our first case! A number thief has mixed up all the numbers. How can we make sense of them? What makes the number 27 different from 72?" },
                { type: 'title', title: 'Demonstration', content: "Let's learn about Place Value. The position of a number changes its value. The '2' in 27 means 2 tens, but the '2' in 72 means 2 ones!" },
                { type: 'activity', section: 'I Do', id: 'd1_ido_1', activityType: 'textbox', prompt: "The clue is 45. Write this in words.", answer: "forty-five" },
                { type: 'activity', section: 'I Do', id: 'd1_ido_2', activityType: 'textbox', prompt: "The clue says 'sixty-two'. Write this as a number.", answer: "62" },
                { type: 'activity', section: 'I Do', id: 'd1_ido_3', activityType: 'mc', prompt: "The word is 'nineteen'. Which number matches?", options: ["91", "19", "90"], answer: "19" },
                { type: 'title', title: 'We Do - Guided Practice', content: "Now, let's solve some cases together! (10 activities)" },
                ...Array.from({ length: 10 }, (_, i) => {
                    const num = 11 + i * 8;
                    const words = ["eleven", "nineteen", "twenty-seven", "thirty-five", "forty-three", "fifty-one", "fifty-nine", "sixty-seven", "seventy-five", "eighty-three"];
                    if (i % 2 === 0) {
                        return { type: 'activity', section: 'We Do', id: `d1_wedo_${i+1}`, activityType: 'textbox', prompt: `Write '${words[i]}' as a number.`, answer: `${num}` };
                    } else {
                        return { type: 'activity', section: 'We Do', id: `d1_wedo_${i+1}`, activityType: 'textbox', prompt: `Write the number ${num} in words.`, answer: words[i] };
                    }
                }),
                { type: 'title', title: 'You Do - Independent Practice', content: "Your turn to be the lead detective! Solve these first few clues. (5 activities)" },
                ...Array.from({ length: 5 }, (_, i) => {
                    const num = 91 + i;
                    const words = ["ninety-one", "ninety-two", "ninety-three", "ninety-four", "ninety-five"];
                     if (i % 2 === 0) {
                        return { type: 'activity', section: 'You Do', id: `d1_youdo_${i+1}`, activityType: 'textbox', prompt: `Write the number ${num} in words.`, answer: words[i] };
                    } else {
                        return { type: 'activity', section: 'You Do', id: `d1_youdo_${i+1}`, activityType: 'textbox', prompt: `Write '${words[i]}' as a number.`, answer: `${num}` };
                    }
                }),
                { type: 'final', content: "You've got the hang of it! Now, head to the Practice Center to solve more cases and sharpen your skills." }
            ],
            practice: {
                level1: [
                    ...Array.from({length: 10}, (_, i) => ({ id: `d1_p1_mc${i}`, type: 'mc', prompt: `Which is "thirty-${i+1 === 10 ? 'nine' : i+1}"?`, options: [`${30+i+1}`, `${13+i}`, `${i+1}3`], answer: `${30+i+1}`})),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d1_p1_tf${i}`, type: 'tf', prompt: `Is "${20+i}" written as "twenty-${["one", "two", "three", "four", "five"][i]}"?`, answer: 'true' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d1_p1_dd${i}`, type: 'drag_match', prompt: `Match the number to the word.`, items: {num: `${45+i}`, word: ["forty-five", "forty-six", "forty-seven", "forty-eight", "forty-nine"][i]}, answer: `${45+i},${["forty-five", "forty-six", "forty-seven", "forty-eight", "forty-nine"][i]}`}))
                ],
                level2: [
                    ...Array.from({length: 7}, (_, i) => ({ id: `d1_p2_mc${i}`, type: 'mc', prompt: `Which number means "eighty-${["six", "seven", "eight", "nine", "one", "two", "three"][i]}"?`, options: [`${80+i}`, `${18+i}`, `${86+i}`], answer: `${86+i}`})),
                    ...Array.from({length: 3}, (_, i) => ({ id: `d1_p2_ms${i}`, type: 'ms', prompt: `Select all the ways to write ${55+i}.`, options: [`fifty-${["five", "six", "seven"][i]}`, `5 tens ${["five", "six", "seven"][i]} ones`, `${50+5+i}`, `forty-${["five", "six", "seven"][i]}`], answer: [`fifty-${["five", "six", "seven"][i]}`,`5 tens ${["five", "six", "seven"][i]} ones`,`${50+5+i}`].join(',') })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d1_p2_cr${i}`, type: 'textbox', prompt: `Write "${["ninety-eight", "seventy-one", "sixty-four", "thirty-two", "one hundred"][i]}" in standard form.`, answer: ["98", "71", "64", "32", "100"][i] })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d1_p2_ds${i}`, type: 'drag_sort', prompt: `Sort the numbers and words into matching pairs.`, items: [`${11+i}`, `${12+i}`, ["eleven", "twelve", "thirteen", "fourteen", "fifteen"][i], ["twelve", "thirteen", "fourteen", "fifteen", "sixteen"][i]], answer: `${11+i},${["eleven", "twelve", "thirteen", "fourteen", "fifteen"][i]},${12+i},${["twelve", "thirteen", "fourteen", "fifteen", "sixteen"][i]}`}))
                ],
                level3: [
                    ...Array.from({length: 5}, (_, i) => ({ id: `d1_p3_mc${i}`, type: 'mc', prompt: `Which number is written as "seventy-three"?`, options: ['37', '73', '703'], answer: '73' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d1_p3_eb${i}`, type: 'eb', promptA: `What is ${81+i} in words?`, promptB: `Which digit is in the tens place?`, answerA: ["eighty-one", "eighty-two", "eighty-three", "eighty-four", "eighty-five"][i], answerB: '8' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d1_p3_cr${i}`, type: 'textbox', prompt: `Write a sentence using the number "${["fifty-six", "twenty-two", "ninety", "fourteen", "sixty-eight"][i]}" in word form.`, answer: 'any sentence' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d1_p3_te${i}`, type: 'textbox', prompt: `Fill in the missing number in word form: ${10+i}, eleven, ___, thirteen.`, answer: 'twelve' }))
                ]
            }
        },
        day2: {
            title: "The Case of the Greater Gator",
            slides: [
                { type: 'title', title: 'Hook', content: "Meet Gus the Gator. He's very hungry and ALWAYS wants to eat the bigger number! We'll help Gus decide which number is greater." },
                { type: 'title', title: 'Demonstration', content: "To compare numbers like 42 and 24, look at the tens place first. 4 is bigger than 2, so 42 is greater! The gator's mouth (> or <) always opens to the bigger number." },
                { type: 'activity', section: 'I Do', id: 'd2_ido_1', activityType: 'mc', prompt: "Which symbol should go between 61 and 38? (61 __ 38)", options: ['<', '>', '='], answer: '>' },
