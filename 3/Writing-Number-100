                    ...Array.from({length: 5}, (_, i) => ({ id: `d2_p3_cr${i}`, type: 'textbox', prompt: `Find a number that is > ${50+i} and < ${60+i}.`, answer: 'any valid number' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d2_p3_te${i}`, type: 'drag_numbers', prompt: `Drag numbers to make a true statement: __ > __`, items: [`${10+i}`, `${20+i}`, `${30+i}`], answer: 'any valid combo' }))
                ]
            }
        },
        day3: {
            title: "The Line-Up Puzzle",
            slides: [
                { type: 'title', title: 'Hook', content: "Oh no, detectives! Your badge numbers (54, 28, 61) are mixed up. We need to get you in the right order from least to greatest!" },
                { type: 'title', title: 'Demonstration', content: "To order numbers, first find the smallest. Then, find the smallest of what's left. Keep going until they are all in order!" },
                { type: 'activity', section: 'I Do', id: 'd3_ido_1', activityType: 'drag_sort', prompt: "Order these numbers from least to greatest: 71, 17, 43.", items: ['71', '17', '43'], answer: '17,43,71' },
                { type: 'activity', section: 'I Do', id: 'd3_ido_2', activityType: 'drag_sort', prompt: "Order these numbers from least to greatest: 90, 9, 19.", items: ['90', '9', '19'], answer: '9,19,90' },
                { type: 'activity', section: 'I Do', id: 'd3_ido_3', activityType: 'textbox', prompt: "Type these numbers in order from least to greatest: 55, 65, 25.", answer: '25,55,65' },
                { type: 'title', title: 'We Do - Guided Practice', content: "Let's get these numbers in line! (10 activities)" },
                ...Array.from({ length: 10 }, (_, i) => ({ type: 'activity', section: 'We Do', id: `d3_wedo_${i+1}`, activityType: 'textbox', prompt: `Order from least to greatest: ${10+i}, ${50-i*2}, ${30+i*3}.`, answer: [10+i, 50-i*2, 30+i*3].sort((a,b)=>a-b).join(',') })),
                { type: 'title', title: 'You Do - Independent Practice', content: "Time to organize on your own. (5 activities)" },
                ...Array.from({ length: 5 }, (_, i) => ({ type: 'activity', section: 'You Do', id: `d3_youdo_${i+1}`, activityType: 'drag_sort', prompt: `Order from least to greatest: ${88-i}, ${77+i}, ${66-i}.`, items: [`${88-i}`, `${77+i}`, `${66-i}`], answer: [88-i, 77+i, 66-i].sort((a,b)=>a-b).join(',') })),
                { type: 'final', content: "Great organizing! Solve more line-up puzzles in the Practice Center." }
            ],
            practice: {
                level1: [
                    ...Array.from({length: 10}, (_, i) => ({ id: `d3_p1_mc${i}`, type: 'mc', prompt: `Which set is in order?`, options: ['1,2,3', '3,1,2', '2,3,1'], answer: '1,2,3' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d3_p1_tf${i}`, type: 'tf', prompt: `Are ${10+i}, ${20+i}, ${30+i} in order?`, answer: 'true' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d3_p1_dd${i}`, type: 'drag_sort', prompt: `Order these on the number line.`, items: [`${15+i}`, `${5+i}`, `${25+i}`], answer: `${5+i},${15+i},${25+i}` }))
                ],
                level2: [
                    ...Array.from({length: 7}, (_, i) => ({ id: `d3_p2_mc${i}`, type: 'mc', prompt: `Which set is NOT in order?`, options: ['10,20,30', '45,54,60', '78,76,77'], answer: '78,76,77' })),
                    ...Array.from({length: 3}, (_, i) => ({ id: `d3_p2_ms${i}`, type: 'ms', prompt: `Select all sets that are ordered correctly.`, options: ['1,2,3', '10,8,6', '22,33,44', '99,1,50'], answer: '1,2,3,22,33,44' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d3_p2_cr${i}`, type: 'textbox', prompt: `Type in order: 81, 18, 48, 84`, answer: '18,48,81,84' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d3_p2_ds${i}`, type: 'drag_sort', prompt: `Order these four numbers.`, items: [`${1+i}`,`${100-i}`,`${50+i}`,`${25-i}`], answer: [1+i, 100-i, 50+i, 25-i].sort((a,b)=>a-b).join(',') }))
                ],
                level3: [
                    ...Array.from({length: 5}, (_, i) => ({ id: `d3_p3_mc${i}`, type: 'mc', prompt: `Find the number that fits: 45, 54, __, 72`, options: ['63', '43', '73'], answer: '63' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d3_p3_eb${i}`, type: 'eb', promptA: `Order 91, 19, 90, 9.`, promptB: `Which number is second?`, answerA: '9,19,90,91', answerB: '19' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d3_p3_cr${i}`, type: 'textbox', prompt: `Create a set of 4 numbers that are in order.`, answer: 'any valid set' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d3_p3_te${i}`, type: 'drag_sort', prompt: `Create a sequence from the number bank.`, items: ['11','99','55','33','77','22'], answer: '11,22,33,55,77,99' }))
                ]
            }
        },
        day4: {
            title: "The Top Secret Countdown",
            slides: [
                { type: 'title', title: 'Hook', content: "Final mission! We've intercepted a secret countdown code: 33, 99, 19. We must put them in order from greatest to least!" },
                { type: 'title', title: 'Demonstration', content: "This is the reverse of yesterday. Now we look for the BIGGEST number first. Then the biggest of the rest, and so on." },
                { type: 'activity', section: 'I Do', id: 'd4_ido_1', activityType: 'drag_sort', prompt: "Order these numbers from greatest to least: 25, 52, 22.", items: ['25', '52', '22'], answer: '52,25,22' },
                { type: 'activity', section: 'I Do', id: 'd4_ido_2', activityType: 'textbox', prompt: "Type in order from greatest to least: 100, 10, 1.", answer: '100,10,1' },
                { type: 'activity', section: 'I Do', id: 'd4_ido_3', activityType: 'drag_sort', prompt: "Order from greatest to least: 48, 84, 44.", items: ['48', '84', '44'], answer: '84,48,44' },
                { type: 'title', title: 'We Do - Guided Practice', content: "Let's master the countdown! (10 activities)" },
                ...Array.from({ length: 10 }, (_, i) => ({ type: 'activity', section: 'We Do', id: `d4_wedo_${i+1}`, activityType: 'textbox', prompt: `Order from greatest to least: ${5+i*5}, ${95-i*5}, ${50+i}.`, answer: [5+i*5, 95-i*5, 50+i].sort((a,b)=>b-a).join(',') })),
                { type: 'title', title: 'You Do - Independent Practice', content: "Final solo mission. (5 activities)" },
                ...Array.from({ length: 5 }, (_, i) => ({ type: 'activity', section: 'You Do', id: `d4_youdo_${i+1}`, activityType: 'drag_sort', prompt: `Order from greatest to least: ${99-i*10}, ${11+i*10}, 50.`, items: [`${99-i*10}`, `${11+i*10}`, `50`], answer: [99-i*10, 11+i*10, 50].sort((a,b)=>b-a).join(',') })),
                { type: 'final', content: "Case closed, detectives! You've mastered numbers to 100. Show off your skills in the final Practice Center challenge." }
            ],
            practice: {
                level1: [
                    ...Array.from({length: 10}, (_, i) => ({ id: `d4_p1_mc${i}`, type: 'mc', prompt: `Which is "twenty-${["one", "two", "three", "four", "five", "six", "seven", "eight", "nine", "ten"][i]}"?`, options: [`${21+i}`, `${12+i}`, `2${i+1}`], answer: `${21+i}`})),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d4_p1_tf${i}`, type: 'tf', prompt: `Is 80, 70, 60 in greatest to least order?`, answer: 'true' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d4_p1_dd${i}`, type: 'drag_sort', prompt: `Order greatest to least.`, items: [`${30+i}`, `${10+i}`, `${20+i}`], answer: `${30+i},${20+i},${10+i}` }))
                ],
                level2: [
                    ...Array.from({length: 7}, (_, i) => ({ id: `d4_p2_mc${i}`, type: 'mc', prompt: `Which set is in order from greatest to least?`, options: ['1,2,3', '30,40,50', '99,55,11'], answer: '99,55,11' })),
                    ...Array.from({length: 3}, (_, i) => ({ id: `d4_p2_ms${i}`, type: 'ms', prompt: `Select all sets in greatest to least order.`, options: ['5,4,3', '10,20,30', '88,8,1', '50,100,1'], answer: '5,4,3,88,8,1' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d4_p2_cr${i}`, type: 'textbox', prompt: `Order from greatest to least: 51, 15, 50, 5`, answer: '51,50,15,5' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d4_p2_ds${i}`, type: 'drag_sort', prompt: `Order greatest to least.`, items: [`${100-i*10}`, `${10+i*10}`, `50`], answer: [100-i*10, 10+i*10, 50].sort((a,b)=>b-a).join(',') }))
                ],
                level3: [
                    ...Array.from({length: 5}, (_, i) => ({ id: `d4_p3_mc${i}`, type: 'mc', prompt: `Sara has 45 stickers. Tom has 54. Who has more?`, options: ['Sara', 'Tom', 'They have the same'], answer: 'Tom' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d4_p3_eb${i}`, type: 'eb', promptA: `Order the scores from highest to lowest: Mia 88, Leo 91, Ava 89.`, promptB: `Who had the highest score?`, answerA: '91,89,88', answerB: 'Leo' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d4_p3_cr${i}`, type: 'textbox', prompt: `Write your own word problem comparing two numbers.`, answer: 'any valid problem' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d4_p3_te${i}`, type: 'drag_sort', prompt: `Create a valid sequence from greatest to least.`, items: ['21','9','59','31','79','2'], answer: '79,59,31,21,9,2' }))
                ]
            }
        }
    };

    let appState = {
        activeTab: 'day1',
        progress: {}
    };

    // --- INITIALIZATION ---
    function init() {
        loadProgress();
        renderApp();
        setupEventListeners();
        navigateToTab(appState.activeTab);
    }

    // --- STATE MANAGEMENT ---
    function saveProgress() {
        localStorage.setItem('numberDetectivesProgress', JSON.stringify(appState));
    }

    function loadProgress() {
        const injectedDataEl = document.getElementById('progress-data');
        if (injectedDataEl && injectedDataEl.textContent.trim()) {
            try {
                const loadedState = JSON.parse(injectedDataEl.textContent);
                appState = loadedState;
                console.log("Loaded progress from injected data.");
                injectedDataEl.textContent = ''; 
                return;
            } catch (e) {
                console.error("Failed to parse injected progress data:", e);
            }
        }
        const savedState = localStorage.getItem('numberDetectivesProgress');
        if (savedState) {
            appState = JSON.parse(savedState);
            console.log("Loaded progress from localStorage.");
        }
    }

    function updateProgress(id, answer, status) {
        const day = appState.activeTab;
        if (!appState.progress[day]) {
            appState.progress[day] = { lesson: {}, practice: {}, streak: 0, currentSlide: 0 };
        }
        const isPractice = document.querySelector(`[data-id="${id}"]`).closest('.practice-center');
        const progressArea = isPractice ? 'practice' : 'lesson';
        appState.progress[day][progressArea][id] = { answer, status };
        if (isPractice) {
            updateStreak(status === 'correct');
        }
        saveProgress();
    }
    
    function updateStreak(isCorrect) {
        const dayData = appState.progress[appState.activeTab];
        if (!dayData) return;
        dayData.streak = isCorrect ? (dayData.streak || 0) + 1 : 0;
        const streakEl = document.querySelector(`#${appState.activeTab} .streak-counter`);
        if(streakEl) {
            streakEl.textContent = dayData.streak;
            if (isCorrect && dayData.streak > 0) {
                streakEl.parentElement.classList.add('streak-counter');
                setTimeout(() => streakEl.parentElement.classList.remove('streak-counter'), 300);
            }
        }
        saveProgress();
    }

    // --- RENDERING ---
    function renderApp() {
        const contentArea = document.getElementById('content-area');
        contentArea.innerHTML = ''; // Clear existing content
        for (const dayId in lessonData) {
            const dayData = lessonData[dayId];
            const dayContainerHTML = createDayContainerHTML(dayId, dayData.title);
            contentArea.insertAdjacentHTML('beforeend', dayContainerHTML);
            const dayContainer = document.getElementById(dayId);
            const lessonSlidesContainer = dayContainer.querySelector('.lesson-slides');
            lessonSlidesContainer.innerHTML += dayData.slides.map((slide, index) => createSlideHTML(slide, index, dayData.slides.length, dayId)).join('');
            const practiceContainer = dayContainer.querySelector('.practice-content');
            practiceContainer.innerHTML = createPracticeLevelHTML(dayData.practice.level1);
        }
    }

    function createDayContainerHTML(dayId, title) {
        return `
            <div id="${dayId}" class="tab-content hidden">
                <div class="flex justify-between items-center mb-4 no-print">
                    <h2 class="text-2xl font-bold text-blue-800" id="${dayId}-mode-title">${title}</h2>
                    <button class="mode-toggle-btn bg-yellow-500 hover:bg-yellow-600 text-white font-bold py-2 px-4 rounded-lg" data-day="${dayId}" data-mode="lesson">Go to Practice Center 🔎</button>
                </div>
                <div class="lesson-slides" data-day="${dayId}">
                    <div class="w-full bg-gray-200 rounded-full h-2.5 mb-4 no-print">
                        <div class="bg-green-500 h-2.5 rounded-full progress-bar-fill" style="width: 0%;"></div>
                    </div>
                </div>
                <div class="practice-center hidden" data-day="${dayId}">
                     <div class="flex justify-between items-center mb-4 no-print">
                        <div class="flex border-b-2 border-gray-300">
                            <button data-day="${dayId}" data-level="1" class="practice-level-btn py-2 px-4 font-bold text-gray-600 border-b-4 border-transparent hover:bg-yellow-200 level-active">Level 1: Approaching</button>
                            <button data-day="${dayId}" data-level="2" class="practice-level-btn py-2 px-4 font-bold text-gray-600 border-b-4 border-transparent hover:bg-yellow-200">Level 2: On Level</button>
                            <button data-day="${dayId}" data-level="3" class="practice-level-btn py-2 px-4 font-bold text-gray-600 border-b-4 border-transparent hover:bg-yellow-200">Level 3: Exceeding</button>
                        </div>
                        <div class="text-2xl font-bold text-yellow-600 flex items-center">
                            🔥 <span class="streak-counter ml-2">0</span>
                        </div>
                    </div>
                    <div class="practice-content"></div>
                </div>
            </div>
        `;
    }

    function createSlideHTML(slide, index, totalSlides, dayId) {
        let contentHTML = '';
        if (slide.type === 'title') {
            contentHTML = `<div class="text-center p-8 bg-yellow-100 rounded-lg">
                <h3 class="text-2xl font-bold text-yellow-700 mb-2">${slide.title}</h3>
                <p class="text-xl">${slide.content}</p>
            </div>`;
        } else if (slide.type === 'activity') {
            contentHTML = createActivityHTML(slide);
        } else if (slide.type === 'final') {
            contentHTML = `<div class="text-center p-8">
                <p class="text-xl mb-4">${slide.content}</p>
                <button class="go-to-practice-btn bg-blue-600 hover:bg-blue-700 text-white font-bold py-3 px-6 rounded-lg text-2xl" data-day="${dayId}">Go to Practice Center 🔎</button>
            </div>`;
        }
        return `
            <div class="slide p-4 border rounded-lg bg-gray-50 shadow-inner" data-slide-index="${index}">
                <div class="activity-content">${contentHTML}</div>
                <div class="flex justify-between items-center mt-6 no-print">
                    <button class="prev-slide-btn bg-gray-300 hover:bg-gray-400 font-bold py-2 px-4 rounded" ${index === 0 ? 'disabled' : ''}>&larr; Previous</button>
                    <span class="text-gray-500">${index + 1} / ${totalSlides}</span>
                    <button class="next-slide-btn bg-gray-300 hover:bg-gray-400 font-bold py-2 px-4 rounded" ${index === totalSlides - 1 ? 'disabled' : ''}>Next &rarr;</button>
                </div>
            </div>
        `;
    }

    function createActivityHTML(activity) {
        let interactionHTML = '';
        switch (activity.type || activity.activityType) {
            case 'textbox':
                interactionHTML = `<textarea class="w-full p-2 border-2 border-gray-300 rounded-lg text-xl focus:border-blue-500 focus:ring-blue-500" rows="1" placeholder="Type your answer here..."></textarea>`;
                break;
            case 'mc':
                interactionHTML = activity.options.map(opt => `
                    <label class="block my-2 p-3 border rounded-lg hover:bg-yellow-100 cursor-pointer text-xl">
                        <input type="radio" name="${activity.id}" value="${opt}" class="mr-2"> ${opt}
                    </label>
                `).join('');
                break;
            case 'ms':
                 interactionHTML = activity.options.map(opt => `
                    <label class="block my-2 p-3 border rounded-lg hover:bg-yellow-100 cursor-pointer text-xl">
                        <input type="checkbox" name="${activity.id}" value="${opt}" class="mr-2"> ${opt}
                    </label>
                `).join('');
                break;
            case 'tf':
                interactionHTML = ['True', 'False'].map(opt => `
                    <label class="block my-2 p-3 border rounded-lg hover:bg-yellow-100 cursor-pointer text-xl">
                        <input type="radio" name="${activity.id}" value="${opt.toLowerCase()}" class="mr-2"> ${opt}
                    </label>
                `).join('');
                break;
            case 'drag_sort':
                interactionHTML = `
                    <div class="flex justify-around items-center bg-gray-100 p-4 rounded-lg mb-4">
                        <p class="font-bold mr-4">Items:</p>
                        <div class="drag-source flex gap-2 flex-wrap">
                            ${activity.items.map(item => `<div class="drag-item bg-yellow-400 p-2 rounded shadow" draggable="true">${item}</div>`).join('')}
                        </div>
                    </div>
                    <p class="font-bold mb-2">Order them here:</p>
                    <div class="drop-zone flex gap-2 w-full rounded-lg bg-gray-50"></div>
                `;
                break;
            case 'drag_match':
                 interactionHTML = `
                    <div class="flex justify-around items-center p-4">
                        <div class="drag-source flex flex-col gap-2">
                             <div class="drag-item bg-yellow-400 p-2 rounded shadow" draggable="true">${activity.items.num}</div>
                             <div class="drag-item bg-blue-400 text-white p-2 rounded shadow" draggable="true">${activity.items.word}</div>
                        </div>
                        <div class="flex flex-col gap-2">
                            <div class="drop-zone w-32 h-12 rounded-lg bg-gray-50"></div>
                            <div class="drop-zone w-32 h-12 rounded-lg bg-gray-50"></div>
                        </div>
                    </div>
                 `;
                 break;
        }
        return `
            <div class="p-6 bg-white rounded-lg shadow-md" data-id="${activity.id}" data-answer="${activity.answer}">
                <p class="text-xl mb-4">${activity.prompt}</p>
                <div class="interaction-area">${interactionHTML}</div>
                <div class="mt-4 flex items-center">
                    <button class="check-answer-btn bg-blue-500 hover:bg-blue-600 text-white font-bold py-2 px-4 rounded-lg no-print">Check Answer</button>
                    <div class="feedback ml-4 font-bold text-lg"></div>
                </div>
            </div>
        `;
    }

    function createPracticeLevelHTML(questions) {
        if (!questions || questions.length === 0) return '<p>No practice questions for this level yet.</p>';
        return questions.map(q => createActivityHTML(q)).join('<hr class="my-4">');
    }

    // --- NAVIGATION & UI LOGIC ---
    function navigateToTab(tabId) {
        appState.activeTab = tabId;
        document.querySelectorAll('.tab-content').forEach(c => c.classList.add('hidden'));
        document.getElementById(tabId).classList.remove('hidden');
        document.querySelectorAll('.tab-btn').forEach(b => b.classList.remove('tab-active'));
        document.querySelector(`.tab-btn[data-tab="${tabId}"]`).classList.add('tab-active');
        const dayProgress = appState.progress[tabId];
        const slideIndex = dayProgress ? (dayProgress.currentSlide || 0) : 0;
        showSlide(tabId, slideIndex);
        saveProgress();
    }
    
    function showSlide(day, index) {
        const slides = document.querySelectorAll(`#${day} .lesson-slides .slide`);
        if (slides.length === 0) return;
        slides.forEach(slide => slide.classList.remove('active'));
        slides[index].classList.add('active');
        if (!appState.progress[day]) appState.progress[day] = { lesson: {}, practice: {}, streak: 0, currentSlide: 0 };
        appState.progress[day].currentSlide = index;
        const progressBar = document.querySelector(`#${day} .progress-bar-fill`);
        if (progressBar) {
            progressBar.style.width = `${((index + 1) / slides.length) * 100}%`;
        }
        saveProgress();
    }

    function toggleMode(day) {
        const lessonSlides = document.querySelector(`#${day} .lesson-slides`);
        const practiceCenter = document.querySelector(`#${day} .practice-center`);
        const toggleBtn = document.querySelector(`.mode-toggle-btn[data-day="${day}"]`);
        const titleEl = document.getElementById(`${day}-mode-title`);
        const isLessonMode = toggleBtn.dataset.mode === 'lesson';
        if (isLessonMode) {
            lessonSlides.classList.add('hidden');
            practiceCenter.classList.remove('hidden');
            toggleBtn.textContent = 'Back to Lesson 📖';
            toggleBtn.dataset.mode = 'practice';
            titleEl.textContent = lessonData[day].title + " - Practice Center";
        } else {
            lessonSlides.classList.remove('hidden');
            practiceCenter.classList.add('hidden');
            toggleBtn.textContent = 'Go to Practice Center 🔎';
            toggleBtn.dataset.mode = 'lesson';
            titleEl.textContent = lessonData[day].title;
        }
    }

    // --- EVENT HANDLING ---
    function setupEventListeners() {
        document.querySelector('nav').addEventListener('click', (e) => {
            if (e.target.matches('.tab-btn')) {
                navigateToTab(e.target.dataset.tab);
            }
        });

        document.getElementById('content-area').addEventListener('click', (e) => {
            const parentContainer = e.target.closest('.p-6.bg-white');
            if (e.target.matches('.check-answer-btn')) {
                const correctAnswer = parentContainer.dataset.answer;
                const activityId = parentContainer.dataset.id;
                const feedbackEl = parentContainer.querySelector('.feedback');
                let userAnswer;
                let isCorrect = false;
                
                const mcInput = parentContainer.querySelector('input[type="radio"]:checked');
                const msInputs = parentContainer.querySelectorAll('input[type="checkbox"]:checked');
                const textInput = parentContainer.querySelector('textarea');
                const dropZone = parentContainer.querySelector('.drop-zone');

                if (mcInput) {
                    userAnswer = mcInput.value;
                    isCorrect = userAnswer === correctAnswer;
                } else if (msInputs && msInputs.length > 0) {
                    userAnswer = Array.from(msInputs).map(i => i.value).sort().join(',');
                    isCorrect = userAnswer === correctAnswer.split(',').sort().join(',');
                } else if (textInput) {
                    userAnswer = textInput.value.trim().toLowerCase().replace(/\s*,\s*/g, ',');
                    isCorrect = userAnswer.replace(/[\s-]/g, '') === correctAnswer.replace(/[\s-]/g, '');
                } else if (dropZone) {
                    userAnswer = Array.from(dropZone.children).map(child => child.textContent).join(',');
                    isCorrect = userAnswer === correctAnswer;
                }

                feedbackEl.textContent = isCorrect ? positiveFeedback[Math.floor(Math.random() * positiveFeedback.length)] : negativeFeedback[Math.floor(Math.random() * negativeFeedback.length)];
                feedbackEl.className = `feedback ml-4 font-bold text-lg ${isCorrect ? 'correct-feedback' : 'incorrect-feedback'}`;
                updateProgress(activityId, userAnswer, isCorrect);
            }
            if (e.target.matches('.next-slide-btn, .prev-slide-btn')) {
                const day = e.target.closest('.tab-content').id;
                const currentSlide = e.target.closest('.slide');
                const currentIndex = parseInt(currentSlide.dataset.slideIndex);
                const nextIndex = e.target.matches('.next-slide-btn') ? currentIndex + 1 : currentIndex - 1;
                showSlide(day, nextIndex);
            }
            if (e.target.matches('.mode-toggle-btn, .go-to-practice-btn')) {
                toggleMode(e.target.dataset.day);
            }
            if (e.target.matches('.practice-level-btn')) {
                const day = e.target.dataset.day;
                const level = e.target.dataset.level;
                const practiceContainer = document.querySelector(`#${day} .practice-content`);
                const levelData = lessonData[day].practice[`level${level}`];
                practiceContainer.innerHTML = createPracticeLevelHTML(levelData);
                document.querySelectorAll(`#${day} .practice-level-btn`).forEach(btn => btn.classList.remove('level-active'));
                e.target.classList.add('level-active');
            }
        });

        // Drag and Drop listeners
        let draggedItem = null;
        document.getElementById('content-area').addEventListener('dragstart', e => {
            if (e.target.matches('.drag-item')) {
                draggedItem = e.target;
                setTimeout(() => { e.target.style.opacity = '0.5'; }, 0);
            }
        });
        document.getElementById('content-area').addEventListener('dragend', e => {
            if (draggedItem) {
                setTimeout(() => {
                    draggedItem.style.opacity = '1';
                    draggedItem = null;
                }, 0);
            }
        });
        document.getElementById('content-area').addEventListener('dragover', e => e.preventDefault());
        document.getElementById('content-area').addEventListener('drop', e => {
            e.preventDefault();
            const dropTarget = e.target.closest('.drop-zone, .drag-source');
            if (dropTarget && draggedItem) {
                dropTarget.appendChild(draggedItem);
            }
        });

        document.getElementById('save-html-btn').addEventListener('click', saveAsHtml);
        document.getElementById('save-pdf-btn').addEventListener('click', saveAsPdf);
    }
    
    // --- SAVING & EXPORTING ---
    function saveAsHtml() {
        const newDoc = document.cloneNode(true);
        const progressScript = newDoc.getElementById('progress-data');
        progressScript.textContent = JSON.stringify(appState);
        const blob = new Blob([newDoc.documentElement.outerHTML], { type: 'text/html' });
        const a = document.createElement('a');
        a.href = URL.createObjectURL(blob);
        a.download = 'number_detectives_progress.html';
        a.click();
        URL.revokeObjectURL(a.href);
    }

    async function saveAsPdf() {
        const { jsPDF } = window.jspdf;
        const pdf = new jsPDF('p', 'pt', 'a4');
        const contentArea = document.getElementById('app-container');
        const pdfWidth = pdf.internal.pageSize.getWidth();
        
        const originalAlert = window.alert;
        window.alert = (message) => console.log(`PDF Export Alert: ${message}`); // Suppress alert for PDF generation
        
        const clone = contentArea.cloneNode(true);
        clone.style.position = 'absolute';
        clone.style.left = '-9999px';
        clone.style.width = '800px';
        document.body.appendChild(clone);

        clone.querySelectorAll('.activity-content').forEach(activity => {
            const activityId = activity.querySelector('[data-id]')?.dataset.id;
            const dayProgress = appState.progress[appState.activeTab];
            if (!dayProgress || !activityId) return;
            const progress = {...dayProgress.lesson, ...dayProgress.practice}[activityId];
            if (!progress) return;
            const interactionArea = activity.querySelector('.interaction-area');
            if (interactionArea) {
                const staticAnswer = document.createElement('p');
                staticAnswer.className = `text-lg p-2 rounded-lg border ${progress.status === 'correct' ? 'bg-green-100 border-green-400' : 'bg-red-100 border-red-400'}`;
                staticAnswer.innerHTML = `<strong>Your Answer:</strong> ${progress.answer || '<em>No answer saved</em>'}`;
                interactionArea.innerHTML = '';
                interactionArea.appendChild(staticAnswer);
            }
        });

        clone.querySelectorAll('.no-print').forEach(el => el.style.display = 'none');
        
        const canvas = await html2canvas(clone, { scale: 2, logging: false });
        const imgData = canvas.toDataURL('image/png');
        const imgProps = pdf.getImageProperties(imgData);
        const imgHeight = (imgProps.height * pdfWidth) / imgProps.width;
        let heightLeft = imgHeight;
        let position = 0;
        const pdfHeight = pdf.internal.pageSize.getHeight();

        pdf.addImage(imgData, 'PNG', 0, position, pdfWidth, imgHeight);
        heightLeft -= pdfHeight;
        while (heightLeft > 0) {
            position = heightLeft - imgHeight;
            pdf.addPage();
            pdf.addImage(imgData, 'PNG', 0, position, pdfWidth, imgHeight);
            heightLeft -= pdfHeight;
        }
        pdf.save('number_detectives_work.pdf');
        document.body.removeChild(clone);
        window.alert = originalAlert; // Restore alert
    }

    // --- START THE APP ---
    init();
});
</script>


</body></html>
