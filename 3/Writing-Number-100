                    ...Array.from({length: 5}, (_, i) => ({ id: `d2_p1_tf${i}`, type: 'tf', prompt: `Is ${50+i} > ${30+i}?`, answer: 'true' })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d2_p1_dd${i}`, type: 'drag_symbol', prompt: `Drag the correct symbol between the numbers.`, nums: [`${40+i}`, `${45-i}`], answer: '>' }))
                ],
                level2: [
                    ...Array.from({length: 7}, (_, i) => ({ id: `d2_p2_mc${i}`, type: 'mc', prompt: `Compare ${90-i} and ${88+i}.`, options: ['<', '>', '='], answer: (90-i > 88+i) ? '>' : '<' })),
                    ...Array.from({length: 3}, (_, i) => ({ id: `d2_p2_ms${i}`, type: 'ms', prompt: `Select all true statements.`, options: [`45 > 44`, `32 < 23`, `88 = 88`, `19 > 91`], answer: `45 > 44,88 = 88` })),
                    ...Array.from({length: 5}, (_, i) => ({ id: `d2_p2_cr${i}`, type: 'textbox', prompt: `Type the correct symbol: ${91+i} __ ${89+i}`, answer: '>' })),
