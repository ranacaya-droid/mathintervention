                { type: 'title', title: 'You Do - Independent Practice', content: "You're on your own, detective! (5 activities)" },
                ...Array.from({ length: 5 }, (_, i) => ({ type: 'activity', section: 'You Do', id: `d2_youdo_${i+1}`, activityType: 'mc', prompt: `Compare ${80+i} and ${85-i}.`, options: ['<', '>', '='], answer: (80+i > 85-i) ? '>' : ((80+i < 85-i) ? '<' : '=') })),
                { type: 'final', content: "You're an expert gator guide! Head to the Practice Center for more comparison cases." }
            ],
            practice: {
                level1: [
